{
  "name": "quotes-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@tanstack/react-query": "^5.80.7",
    "axios": "^1.10.0",
    "expo": "~53.0.11",
    "expo-router": "~5.1.0",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.515.0",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-url-polyfill": "^2.0.0",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "axios-mock-adapter": "^2.1.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.7",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ],
    "coveragePathIgnorePatterns": [
      "/constants/",
      "/app/_layout.tsx",
      "/types/"
    ]
  },
  "private": true
}
